{
  "name": "google-form-getter",
  "scripts": {
    "clean": "rimraf dist",
    "parcel:watch": "parcel watch src/index.html --out-dir dist",
    "parcel:build": "parcel build src/index.html --out-dir dist --public-url './'",
    "serve": "nodemon src/server.js",
    "watch": "run-p parcel:watch serve",
    "dev": "run-s clean watch",
    "build": "run-s clean parcel:build",
    "gh-pages": "gh-pages -d dist",
    "surge": "surge -d https://google-former.surge.sh dist",
    "netlify": "netlify deploy --prod --dir dist",
    "production": "node src/server.js",
    "deploy": "run-s build gh-pages surge netlify",
    "start": "run-s build production"
  },
  "devDependencies": {
    "gh-pages": "^3.1.0",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "netlify-cli": "^2.69.4",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.29.0",
    "surge": "^0.21.7",
    "sweetalert2": "^10.10.4"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "node-fetch": "^2.6.1"
  },
  "engines": {
    "node": "12.x"
  }
}
